{
  "name": "auto-unit-test",
  "displayName": "Auto Unit Test",
  "description": "For generate unit test",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.95.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:php"
  ],
  "main": "src/extension.js",
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "Auto Unit Test",
      "properties": {
        "auto-unit-test.apiKey": {
          "type": "string",
          "default": "",
          "description": "API Key untuk koneksi dengan website pembuatan laporan"
        }
      }
    },
    "commands": [
      {
        "command": "auto-unit-test.testController",
        "title": "CyberTest: Test Function"
      },
      {
        "command": "auto-unit-test.runUnitTest",
        "title": "CyberTest: Menjalankan Unit Test"
      },
      {
        "command": "auto-unit-test.testApiController",
        "title": "CyberTest: Test API Controller"
      },
      {
        "command": "auto-unit-test.updateApiKey",
        "title": "CyberTest: Perbaharui Key"
      },
      {
        "command": "auto-unit-test.deleteKey",
        "title": "CyberTest: Hapus Key"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.9",
    "@types/node": "20.x",
    "@types/vscode": "^1.95.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.13.0"
  },
  "dependencies": {
    "auto-unit-test": "file:",
    "axios": "^1.7.9",
    "dotenv": "^16.4.7"
  }
}